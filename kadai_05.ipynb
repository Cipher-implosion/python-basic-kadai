      "source": [
        "'''\n",
        "上辺：10cm\n",
        "下辺：20cm\n",
        "高さ：5cm\n",
        "\n",
        "公式：台形の面積 =（上辺＋下辺）× 高さ ÷ 2\n",
        "'''\n",
        "\n",
        "# 上辺を定義する\n",
        "top = 10\n",
        "\n",
        "#下辺を定義する\n",
        "bottom = 20\n",
        "\n",
        "# 高さを定義する\n",
        "height = 5\n",
        "\n",
        "# 台型の面積を求める\n",
        "trapezoid = (top + bottom) * height / 2\n",
        "\n",
        "# 結果を表示する\n",
        "print(f\"{trapezoid}㎠\")"
      ]
    }
