        "# randomモジュールをインポート\n",
        "import random\n",
        "\n",
        "# varに1～100までのランダムな整数を入れる\n",
        "var = random.randint(1, 100)\n",
        "print(var)\n",
        "\n",
        "# 条件\n",
        "if var % 15 == 0:\n",
        "  print(\"FizzBazz\")\n",
        "if var % 3 == 0:\n",
        "  print(\"Fizz\")\n",
        "if var % 5 == 0:\n",
        "  print(\"Bazz\")\n",
        "else:\n",
        "  print(var)\n"
      ]
    }
